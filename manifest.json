{
  "name": "Block Links",
  "description": "Say No To Phishing by blocking link clicks",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/images/myicon.png",
    "48": "/images/myicon.png",
    "128": "/images/myicon.png"
  },
  "permissions" : [
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js","script.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/myicon.png"
    }
  }
}

